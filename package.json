{
  "name": "space-log",
  "description": "A simple utility for logging tabular data to the terminal.",
  "version": "1.1.0",
  "type": "module",
  "main": "./lib/index.cjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "files": [
    "lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib --out-file-extension .cjs && cp src/index.mjs lib/",
    "publishLib": "yarn test --coverage && yarn build && npm publish",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-modules-commonjs": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "babel-jest": "29.3.1",
    "jest": "29.3.1",
    "rimraf": "3.0.2"
  },
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/01taylop/space-log"
  },
  "keywords": [
    "console",
    "log",
    "space-log",
    "spaced-log",
    "tabular-log",
    "table-log"
  ],
  "license": "ISC"
}
