{
  "name": "space-log",
  "description": "A simple utility for logging tabular data to the terminal.",
  "version": "1.0.0-alpha.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/01taylop/space-log"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "files": [
    "lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib --out-file-extension .cjs && cp src/index.mjs lib/",
    "publishLib": "yarn build && npm publish",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/plugin-transform-modules-commonjs": "7.17.9",
    "@babel/preset-env": "7.17.10",
    "babel-jest": "28.1.0",
    "jest": "28.1.0",
    "rimraf": "3.0.2"
  },
  "keywords": [
    "space-log",
    "spaced-log",
    "tabular-log",
    "table-log"
  ],
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "license": "ISC"
}
